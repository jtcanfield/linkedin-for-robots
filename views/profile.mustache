 	<body>
    <div class="people-list">
      <div class="profileholder">
        <div class="imageholder">
          <img src="{{avatar}}">
        </div>
        <div id="profileinfo">
          <h1><br>{{name}}</h1>
          {{#address.city}}
            <p>{{address.city}}, {{address.country}} | {{job}}, {{company}}<br>{{email}} | {{phone}}</p>
          {{/address.city}}
          {{^address.city}}
            <p>{{address.country}} | {{job}}, {{company}}<br>{{email}} | {{phone}}</p>
          {{/address.city}}
          {{#university}}
            <h3>Education</h3>
            <p>{{university}}</p>
          {{/university}}
          {{^university}}
          {{/university}}
          <strong class="inline">Skills</strong><br>
          <p class="inline">
            {{#skills}}
            {{.}}<br>
            {{/skills}}
          </p>
        </div>
      </div>
    </div>
    <br><br><br><br>
    <a href="/index"><h2>Go Back</h2></a>
 	</body>
 </html>
